{
	"window.zoomLevel": 0,
	"workbench.iconTheme": "material-icon-theme",
	"editor.fontFamily": "'Source Code Pro', Menlo, Consolas, Courier, monospace",
	"editor.fontSize": 14,
	"bracket-pair-colorizer-2.colorMode": "Independent",
	"bracket-pair-colorizer-2.highlightActiveScope": true,
	"editor.acceptSuggestionOnEnter": "on",
	"editor.quickSuggestions": {
		"other": true,
		"comments": true,
		"strings": true
	},
	"editor.tabCompletion": "on",
	"editor.insertSpaces": false,
	"editor.tabSize": 4,
	"editor.detectIndentation": false,
	"editor.snippetSuggestions": "top",
	"editor.wrappingIndent": "indent",
	"editor.autoIndent": "brackets",
	"editor.formatOnSave": true,
	"editor.formatOnPaste": true,
	"editor.glyphMargin": true,
	"editor.formatOnSaveTimeout": 5000,
	"editor.wordWrapColumn": 100,
	"editor.wordWrap": "on",
	"emmet.syntaxProfiles": {
		"javascript": "jsx",
		"xml": {
			"attr_quotes": "single"
		}
	},
	"emmet.triggerExpansionOnTab": true,
	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"jsx-sublime-babel-tags": "javascriptreact",
		"twig": "html",
		"php": "html",
		"vue-html": "html",
		"razor": "html",
		"plaintext": "jade"
	},
	"emmet.showAbbreviationSuggestions": true,
	"emmet.showSuggestionsAsSnippets": true,
	"emmet.preferences": {
		"format.forceIndentationForTags": [
			"address",
			"article",
			"aside",
			"audio",
			"blockquote",
			"body",
			"canvas",
			"div",
			"dl",
			"fieldset",
			"figcaption",
			"figure",
			"footer",
			"form",
			"head",
			"header",
			"iframe",
			"main",
			"map",
			"nav",
			"object",
			"ol",
			"p",
			"pre",
			"section",
			"style",
			"table",
			"tbody",
			"td",
			"tfoot",
			"th",
			"thead",
			"tr",
			"ul",
			"video"
		]
	},
	"files.watcherExclude": {
		".idea/**": true,
		".vscode/**": true,
		"**/var/cache/**": true,
		"**/var/logs/**": true,
		"**/var/sessions/**": true,
		"node_modules": true,
		"vendor": true
	},
	"search.exclude": {
		".idea/**": true,
		".vscode/**": true,
		"**/var/cache/**": true,
		"**/var/logs/**": true,
		"**/var/sessions/**": true,
		"node_modules": true,
		"vendor": true
	},
	"files.exclude": {
		"**/._*": true,
		"node_modules": true,
		"vendor": true,
		"*/vendor/": true
	},
	"files.insertFinalNewline": false,
	"files.associations": {
		"*.twig": "twig",
		"*.php_cs": "php",
		".php_cs.dist": "php",
		"*.module": "php",
		"*.php": "php"
	},
	"files.trimTrailingWhitespace": true,
	"files.trimFinalNewlines": true,
	"html.format.contentUnformatted": "pre,code,textarea",
	"html.format.endWithNewline": true,
	"html.format.extraLiners": "head, body, /html",
	"html.format.indentHandlebars": false,
	"html.format.indentInnerHtml": true,
	"html.format.maxPreserveNewLines": null,
	"html.format.preserveNewLines": true,
	"html.format.wrapLineLength": 0,
	"html.format.wrapAttributes": "preserve",
	"importCost.largePackageColor": "#EC3A37F5",
	"importCost.mediumPackageColor": "#B362FF",
	"importCost.smallPackageColor": "#B362FF",
	"git.enabled": false,
	"path-autocomplete.excludedItems": {
		"**/{.DS_Store,.git,.gitignore,node_modules}": {
			"when": "**"
		}
	},
	"path-autocomplete.extensionOnImport": true,
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features",
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features",
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true
	},
	"json.schemas": [
		{
			"fileMatch": [
				"/.eslintrc",
				"*.json"
			],
			"url": "http://json.schemastore.org/eslintrc"
		}
	],
	"javascript.validate.enable": false,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true,
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true
	},
	"[yaml]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"scssFormatter.useTabs": true,
	"scssFormatter.tabWidth": 4,
	"scssFormatter.singleQuote": false,
	"sasslint.enable": true,
	"sasslint.configFile": "/.sass-lint.yml",
	"sasslint.run": "onSave",
	"sasslint.resolvePathsRelativeToConfig": true,
	"sasslint.trace.server": "verbose",
	"php.suggest.basic": false,
	"intelephense.files.exclude": [
		"**/.git/**",
		"**/.svn/**",
		"**/.hg/**",
		"**/CVS/**",
		"**/.DS_Store/**",
		"**/node_modules/**",
		"**/bower_components/**",
		"**/vendor/**/{Test,test,Tests,tests}/**/*Test.php"
	],
	"intelephense.format.enable": true,
	"intelephense.completion.fullyQualifyGlobalConstantsAndFunctions": true,
	"intelephense.completion.triggerParameterHints": false,
	"[php]": {
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true,
		"editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
	},
	"php.validate.enable": true,
	"php.validate.executablePath": "C:\\wamp64\\bin\\php\\php7.4.9\\php.exe",
	"php.executablePath": "\"C:\\wamp64\\bin\\php\\php7.4.9\\php.exe\"",
	// "[twig]": {
	// 	"editor.defaultFormatter": "mblode.twig-language-2"
	// },
	//"twig-language-2.braceLine": true,
	"twigcs.enabled": false,
	"[scss]": {
		"editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter",
		"editor.formatOnSave": true,
		"editor.detectIndentation": false,
		"editor.snippetSuggestions": "top",
		"editor.wrappingIndent": "indent",
		"editor.tabSize": 4,
		"editor.formatOnPaste": true,
		"editor.glyphMargin": true
	},
